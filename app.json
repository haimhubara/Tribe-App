{
  "expo": {
    "name": "app222",
    "slug": "app222",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/whiteLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/whiteLogo.png",
      "resizeMode": "cover",
      "backgroundColor": "#F4F8F7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app222",
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera to take pictures and record videos",
        "NSMicrophoneUsageDescription": "We need access to your microphone to record audio in videos"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you take pictures and share them with your friends."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      "expo-font"
    ]
  }
}
